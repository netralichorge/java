class SwitchCase
{
	public static void main(String args[])
	{
		switch(10)
		{
			case 1:
				System.out.println("Case 1");
				break;
				
			case 2:
				System.out.println("Case 2");
				break;
				
			case 3:
				System.out.println("Case 3");
				break;
			
			default:
				System.out.println("No maching case");
		}
	}
}