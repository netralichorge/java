class Calculator
{
	int a;
	int b;
	
	int addition()
	{
		return a+b;
	}
	
	int substraction()
	{
		return a-b;
	}
}

class Demo23
{
	public static void main(String args[])
	{
		Calculator c1=new Calculator();
		c1.a=12;
		c1.b=10;
		
		int sum=c1.addition();
		int sub=c1.substraction();
		
		System.out.println("Addition is : "+sum);
		System.out.println("Substraction is: "+sub);
	}
}